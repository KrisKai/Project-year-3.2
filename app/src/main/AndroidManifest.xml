<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myprojectyear32">

    <uses-permission android:name="android.permission.RECORD_AUDIO" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/home"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/home"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyProjectYear32">
        <activity android:name=".session.ProfileActivity"></activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".session.LoginActivity" />
        <activity android:name=".session.SignupActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>